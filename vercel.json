{
    "version": 2,
    "routes": [
        {
            "src": "/api/youtube/info",
            "methods": ["GET"],
            "dest": "/api/youtube.js"
        },
        {
            "src": "/api/youtube/download",
            "methods": ["GET"],
            "dest": "/api/youtube.js"
        },
        {
            "src": "/gallery",
            "dest": "/gallery/index.html"
        },
        {
            "src": "/bytescript",
            "dest": "/bytescript/index.html"
        },
        {
            "src": "/projects",
            "dest": "/projects/index.html"
        },
        {
            "src": "/about",
            "dest": "/about/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
} 