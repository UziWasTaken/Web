{
    "version": 2,
    "routes": [
        {
            "src": "/api/youtube/info",
            "methods": ["GET"],
            "dest": "/api/youtube.js"
        },
        {
            "src": "/api/youtube/download",
            "methods": ["GET"],
            "dest": "/api/youtube.js"
        },
        {
            "src": "/tools",
            "dest": "/tools.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
} 