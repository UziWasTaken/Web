{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "**/*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/gallery",
            "methods": ["GET", "POST", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, Authorization"
            },
            "dest": "/api/gallery.js"
        },
        {
            "src": "/api/auth/(.*)",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, Authorization"
            },
            "dest": "/api/auth.js"
        },
        {
            "src": "/account",
            "dest": "/account/index.html"
        },
        {
            "src": "/gallery",
            "dest": "/gallery/index.html"
        },
        {
            "src": "/bytescript",
            "dest": "/bytescript/index.html"
        },
        {
            "src": "/projects",
            "dest": "/projects/index.html"
        },
        {
            "src": "/about",
            "dest": "/about/index.html"
        },
        {
            "src": "/auth",
            "dest": "/auth/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
