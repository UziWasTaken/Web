{
    "version": 2,
    "routes": [
        {
            "src": "/api/v1/users",
            "dest": "/api/users.js"
        },
        {
            "src": "/api/v1/products",
            "dest": "/api/products.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "buildCommand": "next build",
    "outputDirectory": ".next"
} 